@model IEnumerable<AlbumPhoto.Models.Comentariu>
 
 @{
     ViewBag.Title = "Comentarii";
    
     }
 
 <h2>Comentarii</h2>
 
 <table>
    
     <tr>
        
         <th>
                        Text
             
         </th>
         
         <th>
                        Autor
             
         </th>
         
         <th></th>
         
     </tr>
    
    @foreach (var item in Model)
    {
        
    <tr>
        
        <td>
                       @Html.DisplayFor(modelItem => item.Text)
            
        </td>
        
        <td>
                        @Html.DisplayFor(modelItem => item.Autor)
            
        </td>
        
    </tr>
        }
    
    
</table>